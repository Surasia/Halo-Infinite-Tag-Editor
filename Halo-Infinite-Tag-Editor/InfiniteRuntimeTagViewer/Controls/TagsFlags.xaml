            <CheckBox x:Name="flag3" Content="Flag 3" Margin="0,0,0,3"/>
            <CheckBox x:Name="flag4" Content="Flag 4" Margin="0,0,0,3"/>
            <CheckBox x:Name="flag5" Content="Flag 5" Margin="0,0,0,3"/>
            <CheckBox x:Name="flag6" Content="Flag 6" Margin="0,0,0,3"/>
            <CheckBox x:Name="flag7" Content="Flag 7" Margin="0,0,0,3"/>
            <CheckBox x:Name="flag8" Content="Flag 8" Margin="0,0,0,3"/>
        </StackPanel>
    </Border>
</UserControl>
